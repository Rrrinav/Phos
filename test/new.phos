fn make_adder(n: i64) -> |i64| -> i64 {
    let cls : |i64| -> i64 = |x: i64| -> i64 {
        return x + n;
    };
    return cls;
}

let add5  :|i64| -> bool = make_adder(5);

let add10 :|i64| -> i64 = make_adder(10);

print("Closure with capture: add5(3) should be 8");
print(add5(3));

print("Closure with capture: add10(3) should be 13");
print(add10(3));

