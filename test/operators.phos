// 1. Arithmetic
print("Arithmetic:");
print(10 + -5);
print(10.5 - 15.5);
print(-5 * -2);
print(10.0 / 4.0);
print(10 % 3);

// 2. String Concatenation
print("\nString Concat:");
print("Phos " + "Language");

// 3. Comparison
print("\nComparison:");
print(5 > 3);
print("a" != "b");
print("a" == "b");
