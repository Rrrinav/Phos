// 5. Precedence and Casting
let result : f64 = (2 + 3) * (10 - 6) / 2; // 5 * 4 / 2 = 10
print("\nPrecedence (5 * 4 / 2): " + (result as string));
print("Cast to string: " + (42 as string));
